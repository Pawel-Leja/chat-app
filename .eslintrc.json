{
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": "latest"
    },
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn"
    },
    "plugins": ["prettier"]
  }